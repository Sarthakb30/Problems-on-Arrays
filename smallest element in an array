class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        
        int arr[]={13,45,3,24,67};
        int l= arr.length;
        int min=arr[0];
        for(int i=0;i<l;i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
        System.out.println("min value: "+min);
    }
}
